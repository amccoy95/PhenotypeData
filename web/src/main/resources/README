To run this web application, you should specify the location to an
application.properties file with the following defined:


################################################################################
# SPRING MVC
################################################################################

spring.view.suffix = .jsp
spring.view.prefix = /WEB-INF/views/


################################################################################
# SOLR configuration
################################################################################

solr.host = xxxx # URL to the solr server


################################################################################
# Webapp configuration
################################################################################

drupalBaseUrl = https://dev.mousephenotype.org
baseUrl = /data


################################################################################
# Database configuration
################################################################################

datasource.komp2.url = xxxx # Database credentials
datasource.komp2.username = user
datasource.komp2.password = pass

datasource.admintools.url = xxxx # Database credentials
datasource.admintools.username = user
datasource.admintools.password = pass


